<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student" class="vip.ikkyu.lifeCycle.Student" init-method="init">
		<property name="name" value="亡命狂徒--张三"/>
		<property name="age" value="24"/>
	</bean>
	<bean id="myBeanFactoryPostProcessor" class="vip.ikkyu.lifeCycle.MyBeanFactoryPostProcessor"/>
	<bean id="myBeanPostProcessor" class="vip.ikkyu.lifeCycle.MyBeanPostProcessor"/>
</beans>